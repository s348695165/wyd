/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.xian{ background: #f1f1f1; height: 20rpx;}

.select{
  box-sizing: border-box;
  width: 100%;
  height: 110rpx;
  /* border:1px solid #efefef; */
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
 }
 .select_text{
  font-size: 30rpx;
  flex: 1;
  float: right;
    font-size: 32rpx;
    color: #999;
    text-align: right;
    margin-right: 10rpx;
    height: 110rpx;
    line-height: 110rpx
 }
 .select_img{
  width: 40rpx;
  height: 40rpx;
  display: block;
  transition:transform 0.3s;
 }
 .select_img_rotate{
  transform:rotate(90deg); 
 }
 .option_box{
  position: absolute;
  top: 110rpx;
  width: 100%;
  border:1px solid #D0D0D0;
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
  z-index: 10;
  margin-top: 1px;
 }
 .option{
  display: block;
  line-height: 40rpx;
  height: 40rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #D0D0D0;
  padding: 10rpx;
   text-align: center;
 }
 .select_box{ position: relative; float: right; width: 300rpx; }
 .select_box image{    width: 19rpx;
  height: 34rpx;
  float: right;
